~load-ext = $(if $(filter $1,$(~*file-mods*)),$(and $(eval include $(call ~modid-file,$1))1,1))
~get-module = $(call ^Y,$(call ~module-locate,$2,$1),$3,,,,,,,,$`(or $`(if $`(if $`1,,1),!:Mod1 $`(call ^d,$`(call ~sprintf,cannot find %q,$(call ^E,$1)))),$`(if $`(if $`(or $`(filter '%,$`1),$`(filter $`(call ~module-id,$`1),$`(~*file-mods*))),,1),$`(and $`(or $`(~*is-quiet*),$`(info ... compiling $`1))1,$`(if $`(call ~compile-module,$`1,$`(call ~modid-file,$`(call ~module-id,$`1)),),!:Mod1 $`(call ^d,$`(call ~sprintf,compilation of %q failed,$`1)),$`(call ^set,~*file-mods*,$`(call ~append,$`(~*file-mods*),$`(call ~module-id,$`1)))))),$`(call ^Y,$`(call ~module-import,$`1,$`2),$`(call ~module-id,$`1),$`1,,,,,,,$``(or $``(if $``(word 1,$``(foreach pair,$``1,$``(if $``(filter !:EDefn3,$``(word 1,$``(call ~nth,2,$``(subst !=, ,$``(pair))))),1))),$``(if $``(~*is-boot*),!:Mod1 module!0has!0executable!0macros!0(boot=true),$``(call ^require,$``2))),!:Mod0 $``(call ^d,$``2) $``(call ^d,$``3) $``(call ^d,$``1)))))
