define [mod-io]
# Requires: core
# Exports: unescape-path_1 escape-path_1 resolve-path_2 clean-path_1 save-blob_2 hash-file_1 hash-files_1 mkdir-p_1 file-exists?_1 cp-file_2 read-file_1 read-lines_1]or]2]or]3 chmod-file_2 write-file_2 getline_1 fprintf_2]or]more write_2 echo-command_1 shell\#1;~%1;i;1 quote-sh-file_1 quote-sh-arg_1 ioshell_1
$(call ^R,core)
~ioshell = $(and $(if $(filter S,$(SCAM_DEBUG)),$(info shell: $1))1,$(shell $1))
