define [mod-'core]
# Requires: 'runtime 'scam-ct
# Exports: intersperse_2 foldr_3 foldl_3 index-of_2 assoc&x;2;\.;\>;~%-initial`<]subst],1'|@],1'|@1],1>|^d|@[}1}0`[]2]0 assoc-vec&x;2;\.;\>;~assoc-initial`>]^d],1[|1|0`[]2]0 assoc-initial_2 sort-by_2 memoize_1 1!S_1 split_2 uniq_1 see_2 fexpect&x;2;\.;\>;~expect-x`>]~format],1[|1|0`>]~format],1[|2|0`:IL9]4266 assert&x;1;\.;\>;~%-x`[]1]0`:IL9]4217 expect&x;2;\.;\>;~%-x`[]1]0`[]2]0`:IL9]4150 expect-x_3 printf_1]or]more sprintf_1]or]more vsprintf_2 format_1 format-add_1 format_1 dict-collate_1 dict-keys_1 dict-compact_1]or]2 dict-set_3 dict-get_2]or]3 dict-find_2 dict-value&x;1;\.;\>;~nth`']2`<]subst],1'|@=],1'|}],1[|1|0 dict-key_1 append_0]or]more filtersub&x;3;\.;\<;patsubst`[]1]0`[]2]0`<]filter],1[|1|0],1[|3|0 word-index?_1 numeric?_1 while_3 reverse_1 indices&x;1;\.;\>;~%-a`<]words],1[|1|0 vec-or&x;1;\.;\>;^u`<]word],1'|1],1<|filter-out|@'}@1.|@[}1}0 select-words_2 select-vec_2 butlast_1 strip-vec&x;1;\.;\<;filter`']!p`[]1]0 last&x;1;\.;\>;~promote`<]%word],1[|1|0 conj_2 cons_2 concat-vec_1]or]2 xor_2 identity&x;1;\.;\[;1;0 eq?_2
~eq? = $(if $(findstring 1$1,$(findstring 1$2,1$1)),1)
