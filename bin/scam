define [mod-core]
# Requires: 
# Exports: intersperse_2 foldr_3 foldl_3 index-of_2 assoc&i;2;\.;\>;~%-initial`<]subst],1'|@],1'|@1],1>|^d|@[}1}0`[]2]0 assoc-vec&i;2;\.;\>;~assoc-initial`>]^d],1[|1|0`[]2]0 assoc-initial_2 sort-by_2 memoize_1 1!S_1 split_2 uniq_1 see_2 fexpect&i;2;\.;\>;~expect-x`>]~format],1[|1|0`>]~format],1[|2|0`:IL9]4500 assert&i;1;\.;\>;~%-x`[]1]0`:IL9]4451 expect&i;2;\.;\>;~%-x`[]1]0`[]2]0`:IL9]4384 expect-x_3 printf_1]or]more sprintf_1]or]more vsprintf_2 format_1 format-add_1 dict-collate_1 dict-values_1 dict-keys_1 dict-compact_1]or]2 dict-set_3 dict-remove_2 dict-get_2]or]3 dict-find_2 dict-value&i;1;\.;\>;^n`']2`<]subst],1'|@=],1'|}],1[|1|0 dict-key_1 append_0]or]more word-index?_1 numeric?_1 while_3 reverse_1 indices&i;1;\.;\>;~%-a`<]words],1[|1|0 vec-subtract&i;2;\.;\>;~vec-filter`']filter-out`[]2]0`[]1]0 vec-intersect&i;2;\.;\>;~vec-filter`']filter`[]2]0`[]1]0 vec-or&i;1;\.;\>;^n`']1`<]filter-out],1'|@.],1[|1|0 select-words_2 select-vec_2 butlast_1 strip-vec&i;1;\.;\<;filter`']!p`[]1]0 last&i;1;\.;\>;^u`<]%word],1[|1|0 conj_2 cons_2 concat-vec_1]or]2 xor_2 identity&i;1;\.;\[;1;0 eq?_2
