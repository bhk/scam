,$(value $(call ~module-var,$1)))),,,,,,,,,$`(and $`(call ~assert-x,$`1,compile.scm:354)1,$`(call ~promote,$`(patsubst #!0Requires:!0%,%,$`(filter #!0Requires:!0%,$`1))))))
endef
